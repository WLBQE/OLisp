(class trivial
  (member (int bar))
  (constructor bar))

(class circle
  (member (double radius))
  (member (string color))
  (member (trivial foo))
  (constructor radius color foo))

(define ((circle -> double) circle_area)
  (lambda (circle -> double) (c) (* c.radius c.radius 3.14)))

